    //function to hide a div:
    function hideDiv(divName){
        var divObjects = document.getElementsByTagName("div");
        divObjects[divName].style.display = 'none';
    }
    //function to show a div:
    function showDiv(divName){
        var divObjects = document.getElementsByTagName("div");
        divObjects[divName].style.display = '';
    }
    //function for switching between two div-tags
    //attributes: showDivName: name of the div to be shown
    //            hideDivName: name of the div to hide
    function switchDivs(showDivName, hideDivName){
        var divObjects = document.getElementsByTagName("div");
        divObjects[showDivName].style.display = '';
        divObjects[hideDivName].style.display = 'none';
    }
//-->
